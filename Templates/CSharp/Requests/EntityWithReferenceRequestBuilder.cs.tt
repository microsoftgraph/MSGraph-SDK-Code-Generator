<#@ template debug="true" hostspecific="true" language="C#" #>
<#@ include file="EntityRequestBuilder.Base.template.tt"#>
<#

var entity = host.CurrentType.AsOdcmClass();
var namespaceName = this.GetNamespaceName(entity);

#>

namespace <#=namespaceName#>
{
    using System;
    using System.Collections.Generic;
<# if (!this.IsGraphNamespace(namespaceName)) { #>
    using Microsoft.Graph;
<# } #>

    <#=this.GetEntityWithReferenceRequestBuilderClassDefinition(entity)#>
    {

        <#=this.GetEntityWithReferenceRequestBuilderConstructor(entity)#>

        <#=this.GetEntityWithReferenceRequestMethod(entity)#>

        <#=this.GetEntityWithReferenceRequestMethodWithOptions(entity)#>
        
        <#=this.GetEntityReferenceRequestBuilderProperty(entity)#>

    }
}
